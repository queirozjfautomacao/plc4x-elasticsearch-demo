{
  "index_patterns": ["plant-*"],
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0
  },
  "mappings": {
    "properties": {
      "values": {
        "properties": {
          "PreStage": {
            "type": "float"
          },
          "MidStage": {
            "type": "float"
          },
          "PostStage": {
            "type": "float"
          },
          "ConveyorBeltTimestamp": {
            "type": "date"
          }
        }
      },
      "jobName": {
        "type": "keyword"
      },
      "sourceName": {
        "type": "keyword"
      }
    }
  }
}